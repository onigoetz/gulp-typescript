{
  "include": [
    "lib/"
  ],

  "compilerOptions": {
    "lib": ["es2021"],
    "module": "node16",
    "target": "es2021",

    "declaration": true,
    "outDir": "dist",
    "isolatedModules": true,

    //"strict": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "moduleResolution": "node16",
  }
}
